{
// Configure debug launch settings
// Feel free to copy these and modify them for your debugger and MCU
    "version": "0.2.0",
    "projectName": "STM32L432KC",
    "configurations": [
        {
            "name": "STlink launch",
            "cwd": "${workspaceRoot}",
            "executable": "${workspaceRoot}/build/Debug/STM32L432KC.elf",
            "request": "launch",
            "type": "cortex-debug",
            "servertype": "stutil",
            "device": "STM32L432KC",
            "interface": "swd",
            // "preLaunchCommands": ["Build all"], // you can execute command instead of task
            "svdFile": "", // Include svd to watch device peripherals
            "swoConfig": {} // currently (v1.7.0) not supported
        },
        {
            "name": "STlink attach",
            "cwd": "${workspaceRoot}",
            "executable": "${workspaceRoot}/build/Debug/STM32L432KC.elf",
            "request": "attach",
            "type": "cortex-debug",
            "servertype": "stutil",
            "device": "STM32L432KC",
            "interface": "swd",
            // "preLaunchCommands": ["Build all"], // you can execute command instead of task
            //"svdFile": "", // Include svd to watch device peripherals
            "swoConfig": {} // currently (v1.7.0) not dupported
        },
    ]
}